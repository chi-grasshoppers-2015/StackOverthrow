<h1>Create a StackOverthrow Account</h1>

<form action="/users" method="POST">
  <input type="text" name="user[email]" value="<%= @user.email %>" placeholder="Email">
  <input type="text" name="user[username]" value="<%= @user.username %>" placeholder="Username">
  <input type="text" name="user[password]" placeholder="Password">
  <input type="submit" value="Create Account">
</form>
